CREATE TABLE USUARIO (ID INTEGER IDENTITY NOT NULL, creation_date DATETIME NULL, PASSWORD VARCHAR(255) NULL, STATUS VARCHAR(255) NULL, USERNAME VARCHAR(255) NULL, PRIMARY KEY (ID))
CREATE TABLE VAGA (ID INTEGER IDENTITY NOT NULL, LOGRADOURO VARCHAR(255) NULL, Usuario_id VARCHAR(255) NULL, PRIMARY KEY (ID))
CREATE TABLE AUTOR (AUTORID INTEGER NOT NULL, DATA_NASCIMENTO DATETIME NULL, ENDERECO VARCHAR(255) NULL, NOME VARCHAR(255) NULL, SEXO VARCHAR(255) NULL, USUARIO_ID INTEGER NULL, PRIMARY KEY (AUTORID))
ALTER TABLE VAGA ADD CONSTRAINT FK_VAGA_Usuario_id FOREIGN KEY (Usuario_id) REFERENCES USUARIO (usuario)
ALTER TABLE AUTOR ADD CONSTRAINT FK_AUTOR_USUARIO_ID FOREIGN KEY (USUARIO_ID) REFERENCES USUARIO (ID)
